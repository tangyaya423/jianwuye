.my_container{
  height: 100%;
  width: 100%;
  margin: 0 auto;
}
.my_top{
  height: 300rpx;
  width: 100%;
  margin: 0 auto;
}
.my_photo{
  height: 100rpx;
  width: 100rpx;
  float: left;
  border-radius: 50%;
  margin-top:50rpx; 
}
.my_message{
  float: left;
  margin-top: 35rpx;
  margin-left: 50rpx;
}
.my_name{
  display: block;
  float: left;
  width: 100%;
  font-size: 30rpx;
  font-weight: bold;
  margin-top:20rpx; 
}
.my_phone{
  display: block;
  float:left;
   margin-top:10rpx; 
   font-size: 27rpx;
}
.change{
  margin-top: 60rpx;
  margin-left: 50rpx;
}
.other{
  background: rgba(0, 0, 0, 0.3);
  height: 80rpx;
  width: 100%;
  float: left;
  margin-top: 50rpx;
  color: #757879;
  font-size: 30rpx;
}
.other text{
  line-height: 80rpx;
}
.middle,.middle1,.middle2,.middle3{
  width: 100%;
 float: left;
 color: #757879;
 margin-top: 20rpx;
}
.notice{
  height: 60rpx;
  font-size: 27rpx;
  float: left;
  margin-left: 20rpx;
  line-height: 60rpx;
}
.middle image,.middle1 image,.middle2 image,.middle3 image{
  height: 50rpx;
  width: 50rpx;
 display: inline-block;
 float: left;
 margin-left: 15rpx;
}